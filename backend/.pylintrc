[MASTER]
disable=too-few-public-methods,
        missing-module-docstring,
        missing-class-docstring,
        missing-function-docstring,
        no-self-use, no-member, duplicate-code, abstract-method,
        redefined-builtin, invalid-name

extension-pkg-whitelist=pydantic
ignore=tests

# Customize Default Rule Behaviour
ignored-argument-names=authenticated|user|api_key
max-line-length=120
